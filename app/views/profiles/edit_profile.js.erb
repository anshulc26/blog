$("li .tab").each(function() {
	$(this).removeClass("active");
});
$("#edit_profile").addClass("active");
$("#edit_profile").css({'text-decoration':'none', 'outline':'none'});

$("#tab_content").html("<%= escape_javascript(render partial: 'edit_profile')%>");

$(function(){
  history.pushState(null, "", "<%= edit_profile_path( username: current_user.username ) %>")
  $(window).bind("popstate", function(){
    $.getScript(location.href)
  })
})
